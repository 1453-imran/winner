import java.util.Scanner;
public class Giris {


    public static void main(String[] args) {

        Scanner usHesabı = new Scanner(System.in);
        System.out.print("Üssü alınacak Sayiyi Giriniz : ");

        int sayiTaban = usHesabı.nextInt();
        int b = sayiTaban;

        System.out.print("Üs olan sayiyi gir: ");
        int usSayi = usHesabı.nextInt();

        for(int i=1;i<usSayi;i++){
            b = sayiTaban*b;
        }
        System.out.println("Sonuc: "+b);

    }
}
